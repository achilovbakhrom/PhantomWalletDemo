    "is-natural-number": "^4.0.1"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node": "^3.0.0",
    "eslint": "^3.10.0",
    "istanbul": "^0.4.5",
    "istanbul-coveralls": "^1.0.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2"
  },
  "eslintConfig": {
    "extends": "@shinnn/node"
  },
  "react-native": {
